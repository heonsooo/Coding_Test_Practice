    "import numpy as np\n",
    "def solution(arr1, arr2):\n",
    "    arr1 = np.array(arr1) \n",
    "    arr2 = np.array(arr2)\n",
    "    \n",
    "    answer = np.dot(arr1, arr2)\n",
    "    answer = answer.tolist()\n",
    "    \n",
    "    return answer\n",
    "\n",
    "print(solution([[1, 4], [3, 2], [4, 1]],[[3, 3], [3, 3]]))"
   ]
  }
